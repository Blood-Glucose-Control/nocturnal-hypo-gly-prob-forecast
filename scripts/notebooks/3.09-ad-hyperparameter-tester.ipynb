    "from sktime.transformations.series.impute import Imputer\n",
    "\n",
    "# Impute missing values\n",
    "transformer = Imputer(method=\"nearest\")\n",
    "transformer.fit(y)\n",
    "y = transformer.transform(y)\n",
    "\n",
    "y"
