    "def simplify_dict(d):\n",
    "    for key, value in d.items():\n",
    "        if isinstance(value, list):\n",
    "            # Keep only the first element of the list (max length 1)\n",
    "            d[key] = [\n",
    "                value[0]\n",
    "            ]  # If you want to test other params, change this to a different index\n",
    "    return d\n",
