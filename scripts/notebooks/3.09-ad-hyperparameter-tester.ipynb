   "source": [
    "# We'll just tune the hyperparams on the first 100 timepoints for patient 1 since we're just testing\n",
    "y = df.iloc[:100][\n",
    "    [\"bg-0:00\", \"insulin-0:00\"]\n",
    "]  # Only want to test the endogenous data right now\n",
